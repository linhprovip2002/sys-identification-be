{
    "name": "express-boilerplate",
    "version": "1.0.0",
    "description": "express,typescripts boilerplate",
    "main": "src/index.ts",
    "scripts": {
        "start": "ts-node ./src/index.ts",
        "start:nodemon": "./node_modules/nodemon/bin/nodemon.js",
        "start:dev": "nodemon --exec ts-node ./src/index.ts",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint . --ext .ts --fix",
        "format": "prettier --write \"src/**/*.ts\"",
        "start:prod": "npm run build && node ./dist/src/server.js",
        "build": "npx tsc",
        "seed:admin": "ts-node ./src/system/database/seed/seed.admin.ts",
        "seed:products": "ts-node ./src/system/database/seed/seed.products.ts",
        "seed:price": "ts-node ./src/system/database/seed/seed.price.ts",
        "seed:quantity": "ts-node ./src/system/database/seed/seed.quantity.ts"
    },
    "repository": "https://github.com/linhprovip2002/express-boilerplate",
    "author": "linhprovip2002",
    "license": "MIT",
    "private": true,
    "dependencies": {
        "@babel/core": "^7.23.5",
        "@babel/eslint-parser": "^7.23.3",
        "@types/express": "^4.17.21",
        "@types/mongoose": "^5.11.97",
        "@types/node": "^20.10.4",
        "@typescript-eslint/eslint-plugin": "^6.13.2",
        "@typescript-eslint/parser": "^6.13.2",
        "bcryptjs": "^2.4.3",
        "cors": "^2.8.5",
        "csv-parse": "^5.5.5",
        "dotenv": "^16.3.1",
        "eslint": "^8.55.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-import": "^2.29.0",
        "eslint-plugin-prettier": "^5.0.1",
        "express": "^4.18.2",
        "install": "^0.13.0",
        "joi": "^17.11.0",
        "jsonwebtoken": "^9.0.2",
        "lodash": "^4.17.21",
        "mongoose": "^8.0.3",
        "mongoose-delete": "^1.0.2",
        "mongoose-paginate-v2": "^1.8.0",
        "mongoose-slug-plugin": "^2.1.0",
        "nodemailer": "^6.9.13",
        "nodemon": "^3.0.2",
        "prettier": "3.1.0",
        "swagger-ui-express": "^5.0.0",
        "ts-node": "^10.9.2",
        "typescript": "^5.3.3",
        "winston": "^3.11.0"
    }
}
